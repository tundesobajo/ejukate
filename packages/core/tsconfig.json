{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "types": ["reflect-metadata", "node", "jest"],
    "module": "CommonJS",
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2019",
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "allowJs": true,
    "checkJs": false, 
    "strictPropertyInitialization": false
  },
}
